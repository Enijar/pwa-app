{
  "private": true,
  "name": "pwa-app",
  "version": "0.0.1",
  "scripts": {
    "assets:watch": "npx webpack --progress --hide-modules --watch --mode development",
    "assets:build": "npx webpack --progress --hide-modules --mode production",
    "start:dev-assets": "npx webpack-dev-server --mode development",
    "start:dev": "npx nodemon src/server --watch src/server",
    "start": "npx forever start -o storage/out.log -e storage/error.log \"$(pwd)/src/server\""
  },
  "author": "James Craig",
  "license": "ISC",
